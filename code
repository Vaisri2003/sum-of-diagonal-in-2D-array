import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner Sc = new Scanner(System.in);
	    int n = Sc.nextInt();
	    int[][] a = new int[n][n];
	    int i,j,sum=0;
	    System.out.println("Enter matrix values : " + " ");
	    for(i=0;i<n;i++){
	        for(j=0;j<n;j++){
	           a[i][j] = Sc.nextInt();
	           if(i==j){
	               sum=sum+a[i][j];
	           }
	        }
	    }
		System.out.println("Sum of diagonal elements : "+sum);
	}
}
